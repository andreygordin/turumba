{
  "name": "gordin/turumba",
  "type": "project",
  "authors": [
    {
      "name": "Andrey Gordin",
      "email": "andrey@gordin.su"
    }
  ],
  "config": {
    "process-timeout": 0,
    "sort-packages": true
  },
  "require": {
    "php": "^7.4",
    "ext-fileinfo": "^7.4",
    "ext-json": "^7.4",
    "doctrine/migrations": "^2.2",
    "doctrine/orm": "^2.7",
    "guzzlehttp/guzzle": "^7.0",
    "imagine/imagine": "^1.2",
    "kix/mdash": "^0.5.5",
    "laminas/laminas-config-aggregator": "^1.2",
    "monolog/monolog": "^2.1",
    "php-di/php-di": "^6.1",
    "ramsey/uuid": "^4.0",
    "slim/psr7": "^1.1",
    "slim/slim": "^4.5",
    "symfony/console": "^5.0",
    "symfony/filesystem": "^5.1",
    "symfony/property-access": "^5.1",
    "symfony/serializer": "^5.1",
    "symfony/string": "^5.1",
    "symfony/validator": "^5.1",
    "tinify/tinify": "^1.5",
    "twig/twig": "^3.0",
    "voku/arrayy": "^7.6",
    "webmozart/assert": "^1.8"
  },
  "require-dev": {
    "dms/phpunit-arraysubset-asserts": "^0.2.0",
    "overtrue/phplint": "^2.0",
    "phpunit/phpunit": "^9.1",
    "roave/security-advisories": "dev-master",
    "squizlabs/php_codesniffer": "^3.5",
    "vimeo/psalm": "^3.11"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Test\\": "tests/"
    }
  },
  "scripts": {
    "app": "php bin/app.php --ansi",
    "cs-check": "phpcs",
    "cs-fix": "phpcbf",
    "lint": "phplint",
    "psalm": "psalm",
    "test": "phpunit --colors=always"
  }
}
